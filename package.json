    "@loopback/openapi-v3-types": "^1.2.1",
    "@types/faker": "^4.1.12",
    "@types/koa": "^2.13.1",
    "@types/koa-router": "^7.4.1",
    "@types/ramda": "^0.26.44",
    "@types/swagger-parser": "^4.0.3",
    "@types/swagger-schema-official": "^2.0.21",
    "commander": "^2.20.3",
    "deepmerge": "^2.2.1",
    "faker": "^4.1.0",
    "husky": "^5.1.3",
    "koa": "^2.13.1",
    "koa-compose": "^4.1.0",
